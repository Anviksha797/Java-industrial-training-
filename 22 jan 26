Code3:-
// Aggregation & Dependency: Connecting classes, accessing aggregated attributes
// Address
class Address { // class 
    private String city; // instance 
    Address (String city) { // constructor
        this.city = city;
    }
    public String getCity() {
        return city;
    }
}
// Student class (Aggregate Address)
class Student {
    private String name;
    private Address address; // Aggregation accessmodififer ClassName variableName
    
    Student(String name, Address address) {
        this.name = name;
        this.address = address;
    }
    void display() {
        System.out.println(name + " lives " + address.getCity());
    }
}
public class Main
{
	public static void main(String[] args) {
	    Address add = new Address("Delhi");
	    Student s1 = new Student("Aman", add);
	    
	    s1.display();
	}
}

CODE 4 

//Dependency
//one class is dependent on another class temporarily
//no permanent storage is there
class Payment{
    void pay(){
        System.out.println("Payment Successful");
    }
}
class Gpay{
    void processPayment(Payment p)//service provider
    {
        p.pay();
    }
}
public class Main{
    public static void main(String[]args){
        Gpay pay=new Gpay();
        Payment payment=new Payment();
        pay.processPayment(payment);
    }
}
