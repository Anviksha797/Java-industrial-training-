CODE 1 - REFERENCE VARIABLE

class Student{
    void study(){
        system.out.println("Student is studying");
    }
    void sleep(){
        system.out.println("Student is sleeping");
    }
}
public class Main {
    
    public static void main(String[] args) {
        Student s1 = new Student();
        s1.study();
        s1.sleep();
    
    }
}

CODE 2 - MULTIPLE REFERENCE

class Student {
    int marks=80;
}
public class Main
{
    public static void main(String[]args){
        Student s1=new Student();
        System.out.println(s1.marks);
    }
}

CODE 3 - DEREFERENCING
class Student {
    int marks = 80;
}
public class Main
{
    public static void main(String[] args){
        Student s1 = new Student();
        Student s2 = s1;
        s2 = null;
        System.out.println(s2.marks);
    }
}

CODE 4

import java.util.*;
class Student {
    int id;
    Student(int id){
            this.id=id;
        }
    }
    public class Main{
    public static void main (String[] args) {
        //syntax of ArrayList
        //List <datatype> list_name=new Arraylist<>();
        Map<Integer,Student>map = new HashMap<>();
        
        map.put(1,new Student(101));
        map.put(2,new Student(102));
            System.out.println(map.get(1).id);
            System.out.println(map.get(2).id);
            
    
    }
    }
